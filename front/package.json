    "babel-eslint": "^10.0.1",
    "del": "^3.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "gulp": "^4.0.0",
    "gulp-file": "^0.4.0",
    "gulp-string-replace": "^1.1.2",
    "rollup": "^1.1.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-vue": "^4.3.2",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
